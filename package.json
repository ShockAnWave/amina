{
  "name": "discord-js-bot",
  "version": "5.5.0",
  "description": "An open-source, multipurpose discord bot built using discord-js",
  "main": "dist/index.js",
  "type": "module",
  "author": "Vikshan",
  "license": "ISC",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsc",
    "dev": "nodemon",
    "start": "bun --experimental-specifier-resolution=bun dist/index.js",
    "start-replit": "npm run build && node dist/index.js",
    "prepare": "husky",
    "watch": "tsc -w",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "lint": "eslint src --ext .ts",
    "f": "prettier --write \"src/**/*.ts\""
  },
  "homepage": "https://github.com/vixshan/amina#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vixshan/amina.git"
  },
  "bugs": {
    "url": "https://github.com/vixshan/amina/issues/new/choose"
  },
  "dependencies": {
    "@octokit/rest": "^21.0.2",
    "@types/mongoose": "^5.11.97",
    "@vitalets/google-translate-api": "^9.2.0",
    "axios": "^1.7.7",
    "btoa": "^1.2.1",
    "bun": "^1.1.34",
    "common-tags": "^1.8.2",
    "discord-gamecord": "^4.4.2",
    "discord-giveaways": "^6.0.1",
    "discord-together": "^1.3.31",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.5",
    "enhanced-ms": "^3.0.0",
    "fixedsize-map": "^1.1.0",
    "lavalink-client": "^2.4.1",
    "module-alias": "^2.2.3",
    "moment": "^2.30.1",
    "mongoose": "^8.8.1",
    "node-fetch": "^2.7.0",
    "pino": "^9.5.0",
    "pino-pretty": "^11.3.0",
    "pretty-ms": "^7.0.1",
    "sourcebin_js": "^0.0.3-ignore",
    "string-progressbar": "^1.0.4",
    "table": "^6.8.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@types/bun": "^1.1.13",
    "@types/node": "^22.9.0",
    "@types/node-fetch": "^2.6.11",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "eslint": "^9.14.0",
    "eslint-plugin-jsdoc": "^50.4.3",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.1.7",
    "prettier": "3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3"
  },
  "keywords": [
    "discord",
    "discord.js",
    "discord-bot",
    "open-source",
    "bot",
    "multipurpose",
    "multi-purpose-bot"
  ],
  "_moduleAliases": {
    "@root": ".",
    "@handlers": "src/handlers/",
    "@helpers": "src/helpers/",
    "@schemas": "src/database/schemas/",
    "@structures": "src/structures/",
    "@commands": "src/commands/",
    "@/*": "./src/*"
  }
}
